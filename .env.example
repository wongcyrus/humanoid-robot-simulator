# Robot Simulator Environment Configuration
# Copy this file to .env and modify as needed

# Debug logging (enables DEBUG level logging and Flask debug mode)
DEBUG=false

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Server port (default: 5000)
PORT=5000

# Example usage:
# DEBUG=true LOG_LEVEL=DEBUG python app.py
# or 
# export DEBUG=true && export LOG_LEVEL=DEBUG && python app.py
